{{!----------------------------------- paymentMethodChart ------------------------------------------}}


<script>
  const labels1 = [{{#each paymentMethodChart}}"{{_id}}",{{/each}}];

  const data1 = {
    labels: labels1,
    datasets: [{
      label1: 'Payment Modes Overview',
      backgroundColor: ['rgb(255, 204, 255)',
                        'rgb(204, 204, 255)',
                        'rgb(204, 153, 255)'],
      borderColor: 'white',
      data:[{{#each paymentMethodChart}}{{count}},{{/each}}],
    }]
  };

  const config1 = {
    type: 'doughnut',
    data: data1,
    options: {}
  };
</script>

<script>
  const myChart1 = new Chart(
    document.getElementById('myChart1'),
    config1
  );
</script>



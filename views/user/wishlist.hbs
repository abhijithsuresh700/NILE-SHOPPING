{{#if_eq wishlistCount 0}}
<div style="width:100%;height:10rem; justify-content: center;margin-top: 5rem;" class="mt-5"><h1 style="text-align: center;">No wishList Items </h1></div>
{{else}}
<section class="h-100 gradient-custom">
  {{!-- <form action="/proceed-to-pay" method="post">  --}}
  <div class="container py-5">
    <div class="row d-flex justify-content-center my-4">
      <div class="col-md-8">
        <div class="card mb-4">
          <div class="card-header py-3">
            <h5 class="mb-0">Wish List</h5>
          </div>
          {{#each wishListProducts}}
          
          <div class="card-body">
            <!-- Single item -->
            <div class="row">
              <div class="col-lg-2 col-md-12 mb-4 mb-lg-0">
                <!-- Image -->
                <div class="bg-image hover-overlay hover-zoom ripple rounded" data-mdb-ripple-color="light">
                  <img src="/images/{{this.product.images.[0]}}"
                    class="w-100" alt="Blue Jeans Jacket" />
                  <a href="#!">
                    <div class="mask" style="background-color: rgba(251, 251, 251, 0.2)"></div>
                  </a>
                </div>
                <!-- Image -->
              </div>

              <div class="col-lg-4 col-md-6 mb-4 mb-lg-0">
                <!-- Data -->
                <p><strong>{{this.product.productname}}</strong></p>
              </div>

               <div class="col-lg-2 col-md-6 mb-4 mb-lg-0">
                <!-- Data -->
                <p><strong>â‚¹{{this.product.price}}.00</strong></p>
              </div>

              <div class="col-lg-1 col-md-6 mb-4 mb-lg-0">
                <!-- Data -->
                 <button type="button" onclick="deleteWishListProduct('{{this._id}}','{{this.product._id}}')" class="btn btn-light btn-sm me-1 mb-2 ml-5" data-mdb-toggle="tooltip"
                  title="Remove item">
                  <i class="fas fa-trash" style="color:#FF084E;" aria-hidden="true"></i>
                </button>
              </div>

              <div class="col-lg-2 col-md-6 mb-4 mb-lg-0 ml-5">
                <!-- Data -->
               <a href="/add-to-cart/{{this.product._id}}" class="btn btn-info">Add to Cart</a>
              </div>


            </div>
            <!-- Single item -->

            <hr class="my-4" />

          </div>
         
          {{/each}}
        </div>












        <div class="card mb-4 mb-lg-0">
          <div class="card-body">
            <p><strong>We accept</strong></p>
            <img class="me-2" width="45px"
              src="https://mdbcdn.b-cdn.net/wp-content/plugins/woocommerce-gateway-stripe/assets/images/visa.svg"
              alt="Visa" />
            <img class="me-2" width="45px"
              src="https://mdbcdn.b-cdn.net/wp-content/plugins/woocommerce-gateway-stripe/assets/images/amex.svg"
              alt="American Express" />
            <img class="me-2" width="45px"
              src="https://mdbcdn.b-cdn.net/wp-content/plugins/woocommerce-gateway-stripe/assets/images/mastercard.svg"
              alt="Mastercard" />
            <img class="me-2" width="45px"
              src="data:image/png;base64,"
              alt="PayPal acceptance mark" />
          </div>
        </div>
      </div>
    </div>
  </div>
  {{!-- </form> --}}
</section>
{{/if_eq}}




<script>
   function deleteWishListProduct(wishListId, proId) {
  //   event.preventDefault();
  Swal.fire({
    title: "Are you sure?",
    text: "Once deleted, product will be removed from the wishlist!",
    icon: "warning",
   showCancelButton: true,
  confirmButtonColor: '#3085d6',
  cancelButtonColor: '#d33',
  confirmButtonText: 'Yes, delete it!'
  }).then(async(willDelete) => {
    if (willDelete.isConfirmed) {
    await  Swal.fire({
        title:'Deleted!',
      text:'Your file has been deleted.',
      icon:'success',
  })
      $.ajax({
        url: '/deletewishListProduct',
        data: {wishList:wishListId,product:proId},
        method: 'post',
        success:(response)=>{
          if(response) {
           location.href = "/wishList"
          } else {
            Swal.fire("something went wrong")
          }
        }
      })
    } else {
      Swal.fire("Your wishlist product is safe!");
    }
  })
}

</script>






























